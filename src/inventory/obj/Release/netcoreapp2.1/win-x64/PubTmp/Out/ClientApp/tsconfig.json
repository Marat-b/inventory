{
  "compileOnSave": false,
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "system",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmitOnError": false,
    "removeComments": false,
    "target": "es5",
    "skipLibCheck": true,
    "lib": [ "es2017", "dom" ]


  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "bin"
  ]
}
